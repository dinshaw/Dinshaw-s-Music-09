- form_for @user_session, :url => user_session_path do |f| 
  = f.error_messages 

  %p
    = f.label :email
    = f.text_field :email

  %p 
    = f.label :password
    = f.password_field :password
 
  %p
    = f.check_box :remember_me 
    = f.label :remember_me, nil, :class => :left
  
  %p
    = f.submit "Login" 